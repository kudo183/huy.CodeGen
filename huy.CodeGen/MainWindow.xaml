<Window x:Class="huy.CodeGen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:huy.CodeGen"
        xmlns:view="clr-namespace:huy.CodeGen.View"
        mc:Ignorable="d"
        WindowState="Maximized"
        Title="MainWindow" Height="350" Width="525">
    <TabControl>
        <TabItem Header="Dto">
            <local:GenDto/>
        </TabItem>
        <TabItem Header="Controller">
            <local:GenController/>
        </TabItem>
        <TabItem Header="TextManager">
            <local:GenTextManager/>
        </TabItem>
        <TabItem Header="ViewModel">
            <local:GenViewModel/>
        </TabItem>
        <TabItem Header="View">
            <local:GenView/>
        </TabItem>
        <TabItem Header="All">
            <view:GenAll/>
        </TabItem>
    </TabControl>
</Window>
